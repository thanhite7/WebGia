<script>
    function callaisuat(){
        const tiengui = document.getElementById('ccls_money').value.replace(/\D/g,'');
        const kyhan = document.getElementById('ccls_time').value;
        const laisuat = document.getElementById('ccls_percent').value;
        const thoigian = document.getElementById('ccls_time_type').value;
        var option = thoigian==2? 12:365;
        const lai = (tiengui * parseFloat(kyhan) * parseFloat(laisuat) / 100 / parseFloat(option)).toFixed(3);
        
        const tongtien = (parseFloat(tiengui) + parseFloat(lai)).toFixed(3);
        document.getElementById('ccls_tlai').innerHTML = addCommas((tongtien)) + ' VND';
        document.getElementById('ccls_goc').innerHTML = addCommas((tiengui)) + ' VND';
        document.getElementById('ccls_lai').innerHTML = addCommas((lai)) + ' VND';
        document.getElementById('ccls_glai').innerHTML = addCommas((tongtien)) + ' VND';

    }
    var numberInput = ['ccls_money', 'ccls_time', 'ccls_percent']
    
    document.addEventListener("DOMContentLoaded", function() {
        var numberInput = document.getElementById("ccls_money");

            numberInput.addEventListener("input", function() {
                var value = numberInput.value.replace(/\D/g, ''); // Lọc bỏ tất cả các ký tự không phải là số
                var formattedValue = addCommas(value);
                numberInput.value = formattedValue;
            });
    });

   
   
    function addCommas(number) {
    return number.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
}
</script>
